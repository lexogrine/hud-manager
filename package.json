{
  "name": "hud-manager",
  "version": "3.5.2",
  "homepage": "./",
  "description": "Lexogrine HUD Manager",
  "author": "Lexogrine",
  "private": true,
  "dependencies": {
    "@node-steam/vdf": "^2.2.0",
    "@racehub-io/f1-telemetry-client": "^0.2.6",
    "@sentry/node": "^6.13.3",
    "@sentry/tracing": "^6.13.3",
    "@socket.io/component-emitter": "^3.0.0",
    "archiver": "^5.3.0",
    "big-integer": "^1.6.49",
    "bootstrap": "^4.4.1",
    "cors": "^2.8.5",
    "cross-env": "^6.0.3",
    "csgogsi-generator": "^1.0.1",
    "csgogsi-socket": "^2.5.0",
    "decompress-zip": "^0.2.2",
    "del": "^6.0.0",
    "dnd-core": "^14.0.1",
    "dotagsi": "^1.1.4",
    "electron-updater": "^4.3.5",
    "exceljs": "^4.3.0",
    "express": "^4.17.1",
    "fetch-cookie": "^0.11.0",
    "get-port": "^5.1.1",
    "http-proxy-middleware": "^1.0.6",
    "i18next": "^19.8.7",
    "i18next-http-backend": "^1.3.1",
    "immutability-helper": "^3.1.1",
    "internal-ip": "^6.2.0",
    "ip": "^1.1.5",
    "json5": "^2.1.3",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.1",
    "nedb": "^1.8.0",
    "node-abi": "^3.2.0",
    "node-fetch": "^2.6.5",
    "node-media-server": "^2.3.8",
    "portscanner": "^2.2.0",
    "public-ip": "^4.0.3",
    "pug": "^3.0.2",
    "query-string": "^6.12.1",
    "react": "^17.0.2",
    "react-color": "^2.19.3",
    "react-dnd": "^14.0.4",
    "react-dnd-html5-backend": "^14.0.2",
    "react-dom": "^16.13.1",
    "react-i18next": "^11.12.0",
    "react-scripts": "^4.0.0",
    "react-select-country-list": "^2.1.2",
    "reactstrap": "^8.10.0",
    "registry-js": "^1.15.1",
    "request": "^2.88.2",
    "simple-websockets": "^1.1.0",
    "socket.io": "^4.2.0",
    "socket.io-client": "^4.2.0",
    "steam-game-path": "^2.0.1",
    "tough-cookie": "^4.0.0",
    "tough-cookie-file-store": "^2.0.3",
    "typescript": "^4.4.3",
    "uuid": "^3.4.0",
    "ws": "^7.5.5",
    "yargs": "^15.3.1"
  },
  "build": {
    "appId": "com.lexogrine.hudmanager",
    "productName": "Lexogrine HUD Manager",
    "win": {
      "icon": "./dist/build/favicon.ico",
      "target": [
        {
          "target": "NSIS",
          "arch": [
            "x64"
          ]
        }
      ]
    },
    "linux": {
      "icon": "icon.png",
      "target": "tar.xz"
    },
    "directories": {
      "output": "app",
      "buildResources": "assets"
    },
    "nsis": {
      "license": "license.txt"
    },
    "files": [
      "dist/**/*",
      "package.json",
      {
        "from": "dist",
        "to": "build",
        "filter": "**/*"
      },
      "!**/*.asar",
      "!**/app/**/*",
      "!**/app/*",
      "!**/win-unpacked/*",
      "!**/.git/**/*",
      "!**/.git/*"
    ],
    "publish": [
      {
        "provider": "github",
        "owner": "lexogrine",
        "repo": "hud-manager"
      }
    ]
  },
  "main": "dist/electron.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "npm run transpile && react-scripts build && rm -rf dist/build &&  mv build dist",
    "test": "react-scripts test",
    "transpile": "tsc -p tsconfig.electron.json",
    "devElectron": "cross-env DEV=true electron ./",
    "electron": "npm run transpile && npm run devElectron",
    "dev": "concurrently \"cross-env BROWSER=none npm start\" \"npm run electron\"",
    "eject": "react-scripts eject",
    "dist": "electron-builder --publish never",
    "publish": "electron-builder --publish always",
    "compile": "npm run build && npm run dist",
    "compileAndPublish": "npm run build && npm run publish",
    "lint": "eslint . --ext .ts,.tsx",
    "prettier-format": "prettier --config .prettierrc --write **/*.{ts,tsx}"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/archiver": "^5.1.1",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/http-proxy-middleware": "^0.19.3",
    "@types/ip": "^1.1.0",
    "@types/jest": "24.0.18",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/nedb": "^1.8.12",
    "@types/node": "^16.10.3",
    "@types/node-fetch": "^2.5.12",
    "@types/portscanner": "^2.1.1",
    "@types/react": "^17.0.27",
    "@types/react-color": "^3.0.5",
    "@types/react-dom": "^17.0.9",
    "@types/reactstrap": "^8.4.2",
    "@types/request": "^2.48.7",
    "@types/tough-cookie-file-store": "^2.0.1",
    "@types/uuid": "^3.4.10",
    "@types/ws": "^7.4.7",
    "@typescript-eslint/eslint-plugin": "^3.8.0",
    "@typescript-eslint/parser": "^3.8.0",
    "concurrently": "^6.3.0",
    "electron": "^15.1.1",
    "electron-builder": "^22.14.4",
    "electron-rebuild": "^3.2.3",
    "eslint": "^7.32.0",
    "eslint-plugin-react": "^7.26.1",
    "husky": "^4.2.5",
    "prettier": "^2.4.1",
    "sass": "^1.42.1",
    "wait-on": "^3.3.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run prettier-format && npm run lint"
    }
  }
}
